#!/bin/sh 

if $(ipkg status netatalk2 | grep -q installed); then
	echo -e "Netatalk and Netatalk2 are incompatible, and Netatalk2 is installed, uninstall it first.\n
	Your current configuration is incompatible with this version.\n
	Make sure to read the upgrading instructions from netatalk-2 in
	http://netatalk.sourceforge.net/3.0/htmldocs/upgrade.html"
	exit 1
fi